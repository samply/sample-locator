{
  "stsServer": "{authorization-url}/oidc",
  "redirect_url": "{samplelocator-url}",
  "client_id": "{authorization-client-id}",
  "response_type": "id_token token",
  "scope": "openid email profile phone groupNames offline_access",
  "start_checksession": true,
  "silent_renew": true,
  "silent_renew_url": "{samplelocator-url}/silent-renew.html",
  "post_login_route": "/authorized",
  "forbidden_route": "/forbidden",
  "unauthorized_route": "/unauthorized",
  "log_console_warning_active": true,
  "log_console_debug_active": true,
  "disable_iat_offset_validation": true,
  "max_id_token_iat_offset_allowed_in_seconds": 150
}
